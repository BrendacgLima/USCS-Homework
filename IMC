#include <stdio.h>
#include <math.h>

int mmc(int num1, int num2) {

    int mmc, aux, i;

    for (i = 2; i <= num2; i++) {
        aux = num1 * i;
        if ((aux % num2) == 0) {
            mmc = aux;
            i = num2 + 1;
        }
    }
    return mmc;
}

int main()
{
  int mmc, num1,num2;
  
     printf("Digite o primeiro valor:\n");
     scanf("%d",&num1);
     printf("Digite o segundo valor:\n");
     scanf("%d",&num2);
     
    printf("%2.f",&mmc);
    
    
}
