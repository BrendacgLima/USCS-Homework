#include<stdio.h>

int main(){
	 int a,b,c;
	 
		printf("Type three values: ");
		scanf("%d %d %d", &a, &b, &c);
	
	if(a+b>c && a+c>b && b+c>a){
		printf("The three sides create a triangle:\n");
		if(a==b && a==c)
		printf("Equilateral triangle\n");
	else
  	if(a==b||a==c||b==c)
		printf("Isosceles triangle");
	else
		printf("Scalene triangle");
}

	else
	printf("The 3 sides DOESN'T form a triangle");
}
